        public String remove_scope(String[] remove){
            String scope = "";
            for(int i = 0; i < remove.length-1;i++){
                if(i != remove.length-1){
                    if(i == remove.length-2){
                        scope += remove[i];
                    }else{
                        scope += remove[i]+".";
                    }   
                }
            }
            return scope;
        }
        public String nuevoTemporal(){
            String tmp = "t"+contadortemporales;
            contadortemporales++;
            return tmp;
        }
