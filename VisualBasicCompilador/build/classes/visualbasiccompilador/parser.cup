statements ::=	statement:stmnt statements:stmnts       {:if(!foundError){listastatements.add(stmnt);}:}  
		| /* Vacio */                      
;

statement ::=	if_statement:ifStatement                {:if(!foundError){RESULT = ifStatement;} :}
                |while_statement:whileStatement         {:if(!foundError){RESULT = whileStatement;} :}
		|for_statement:forStatement             {:if(!foundError){RESULT = forStatement;} :}
		|do_statement:doStatement               {:if(!foundError){RESULT = doStatement;} :}
                |write_statement:writeStatement         {:if(!foundError){RESULT = writeStatement;} :}
                |read_statement:readStatement           {:if(!foundError){RESULT = readStatement;} :}
                |variable_declaration:var_declare       {:if(!foundError){RESULT = var_declare;} :}
		|expression:expr                        {:if(!foundError){RESULT = expr;}:}
		|TK_RETURN expression:exp               {:if(!foundError){RESULT = new ReturnExpression("Return",exp);}:}
		|TK_EXIT                                {:if(!foundError){RESULT = new ExitStatement("Exit");} :}
;
write_statement ::= TK_WRITE TK_PARIN literal_expression:le TK_PAROUT           {:if(!foundError){RESULT = new WriteStatement("Write",le,null);} :}
                    |TK_WRITE TK_PARIN TK_ID:i TK_PAROUT                        {:if(!foundError){RESULT = new WriteStatement("Write",null,i);}  :}
                    |TK_WRITE TK_PARIN error TK_PAROUT                          {:System.err.println("\t\t\t\t\t  Problem near Console.Write()"); foundError=true;:}
;
read_statement  ::= TK_READ TK_PARIN TK_ID:i TK_PAROUT               {:if(!foundError){RESULT = new ReadStatement("Read",i);} :}
                    |TK_READ TK_PARIN error TK_PAROUT              {:System.err.println("\t\t\t\t\t  Problem near Console.Read().");foundError=true; :}
;
if_statement ::=    TK_IF expression:expr TK_THEN statements:stmnts TK_END TK_IF                                                                {:if(!foundError){RESULT = new IfStatement("If",expr,"Then",null,listastatements,null,null,"End IF");listastatements = new ArrayList();} :}
                    |TK_IF expression:expr TK_THEN statements:stmnts else_statement:elseStmnt TK_END TK_IF                                      {:if(!foundError){RESULT = new IfStatement("If",expr,"Then","Else",listastatements,null,elseStmnt,"End IF");listastatements = new ArrayList();} :}
                    |TK_IF expression:expr TK_THEN statements:stmnts elseif_statements:elseifStmnts TK_END TK_IF                                {:if(!foundError){RESULT = new IfStatement("If",expr,"Then",null,listastatements,listaelseifs,null,"End IF");listastatements = new ArrayList();listaelseifs = new ArrayList();} :}
                    |TK_IF expression:expr TK_THEN statements:stmnts elseif_statements:elseifStmnts else_statement:elseStmnt TK_END TK_IF       {:if(!foundError){RESULT = new IfStatement("If",expr,"Then","Else",listastatements,listaelseifs,elseStmnt,"End IF");listastatements = new ArrayList();listaelseifs = new ArrayList();} :}
                    |TK_IF expression:expr TK_THEN statements:stmnts error                                                                      {:System.err.print("Error in If.");foundError=true; :}
; 

elseif_statements ::=	elseif_statement:elseifStmnt elseif_statements:elseifStmnts                     {:if(!foundError){listaelseifs.add(elseifStmnt);} :}
			|elseif_statement:elseifStmnt                                                   {:if(!foundError){listaelseifs.add(elseifStmnt);}:}
;
else_statement   ::=    TK_ELSE statements:stmnts                   {:if(!foundError){RESULT = new ElseStatement("Else",listastatements);listastatements = new ArrayList();}:}
;
elseif_statement ::=	TK_ELSEIF expression:expr TK_THEN statements:stmnts           {:if(!foundError){ RESULT = new ElseIfStatement("ElseIf",expr,"Then",listastatements);listastatements = new ArrayList();} :}
;


for_statement ::=	TK_FOR variable_declaration:var_declare TK_TO expression:expr statements:stmnts TK_NEXT                         {:if(!foundError){RESULT = new ForStatement("For",var_declare,"To",expr,listastatements,"Next"); listastatements = new ArrayList(); } :}
                        |TK_FOR error TK_TO expression:expr statements:stmnts TK_NEXT                                                   {:System.err.println("Error in variable declaration by For.");foundError=true; :}
                        |TK_FOR variable_declaration:var_declare TK_TO expression:expr error statements:stmnts TK_NEXT                  {:System.err.println("Check syntax inside For.");foundError=true;:}
