data_type ::=	TK_INTEGER          {: if(!foundError){RESULT = "Integer";} :}
		|TK_BOOLEAN         {: if(!foundError){RESULT = "Boolean";} :}
		|TK_STRING          {: if(!foundError){RESULT = "String";} :}
